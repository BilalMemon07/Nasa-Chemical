<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="vendors_report_wizard" model="ir.ui.view">
        <field name="name">Advance to Vendors Report</field>
        <field name="model">vendors.report</field>
        <field name="arch" type="xml">
            <form string="Advance to Vendors Report">
                <group>
                   <group>
                        <field name="date_from"/>
                        <field name="date_to"/>
                        
                        
                    </group>
                   <group>
                        <field name="vendor_ids" widget="many2many_tags"/>
                    </group>
                </group>
                <footer>
                    <button name="print_report" type="object" string="Print" class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <record id="vendors_report_action" model="ir.actions.act_window">
        <field name="name">Advance to Vendors Report</field>
        <field name="res_model">vendors.report</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <menuitem id="vendors_report_id"
              name="Advance to Vendors Report"
              parent="account.menu_finance_reports"
              action="vendors_report_action"
    />
</odoo>